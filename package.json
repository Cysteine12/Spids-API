{
  "name": "spids-api",
  "version": "1.0.0",
  "description": "A student profiling and identification system",
  "main": "dist/app.js",
  "type": "module",
  "scripts": {
    "build": "tsup src/app.ts --format esm --out-dir dist",
    "dev": "tsc --noEmit && tsx --env-file=.env --watch src/app.ts",
    "start": "node dist/app.js",
    "db:migrate": "prisma migrate dev",
    "db:deploy": "prisma migrate deploy",
    "db:generate": "prisma generate",
    "db:seed": "tsx --env-file=.env prisma/seeder.ts",
    "db:studio": "prisma studio",
    "test": "jest"
  },
  "keywords": [],
  "author": "Cysteine - Anettcom Technologies",
  "license": "MIT",
  "dependencies": {
    "@prisma/client": "^6.8.2",
    "bcryptjs": "^3.0.2",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "express-rate-limit": "^7.5.0",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "mysql2": "^3.14.1",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "tsx": "^4.19.4",
    "winston": "^3.17.0",
    "zod": "^3.24.4"
  },
  "devDependencies": {
    "@faker-js/faker": "^9.8.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/cookie-parser": "^1.4.8",
    "@types/cors": "^2.8.18",
    "@types/express": "^5.0.2",
    "@types/express-rate-limit": "^5.1.3",
    "@types/helmet": "^0.0.48",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/morgan": "^1.9.9",
    "@types/node": "^22.15.18",
    "@types/passport": "^1.0.17",
    "@types/passport-jwt": "^4.0.1",
    "@types/winston": "^2.4.4",
    "jest": "^29.7.0",
    "nodemon": "^3.1.10",
    "prisma": "^6.8.2",
    "supertest": "^7.1.1",
    "ts-jest": "^29.3.4",
    "ts-node": "^10.9.2",
    "tsup": "^8.5.0",
    "typescript": "^5.8.3"
  }
}
